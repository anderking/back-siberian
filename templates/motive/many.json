{
	"status": true,
	"message": {
		"code": "{{#? message.code}}",
		"text": "{{#? message.text}}"
	},
	"motives": {
		"{{#each motives}}": {
			"id": "{{#? this.id}}",
			"name": "{{#? this.name}}",
			"description": "{{#? this.description}}",
			"status": "{{#? this.status}}",
			"createdAt": "{{#? this.createdAt}}",
			"updatedAt": "{{#? this.updatedAt}}"
		}
	}
}