{
	"status": true,
	"message": {
		"code": "{{#? message.code}}",
		"text": "{{#? message.text}}"
	},
	"parameter": {
		"id": "{{#? this.parameter.id}}",
		"name": "{{#? this.parameter.name}}",
		"description": "{{#? this.parameter.description}}",
		"status": "{{#? this.parameter.status}}",
		"createdAt": "{{#? this.parameter.createdAt}}",
		"updatedAt": "{{#? this.parameter.updatedAt}}",
		"group": {
			"id": "{{#? this.parameter.Group.id}}",
			"name": "{{#? this.parameter.Group.name}}",
			"description": "{{#? this.parameter.Group.description}}",
			"status": "{{#? this.parameter.Group.status}}"
		},
		"users": {
			"{{#each this.parameter.Users}}": {
				"id": "{{#? this.id}}",
				"firstName": "{{#? this.firstName}}",
				"lastName": "{{#? this.lastName}}",
				"imageUrl": "{{#? this.imageUrl}}",
				"gender": "{{#? this.gender}}",
				"note": "{{#? this.note}}",
				"status": "{{#? this.status}}",
				"createdAt": "{{#? this.createdAt}}",
				"updatedAt": "{{#? this.updatedAt}}"
			}
		},
		"promotions": {
			"{{#each this.parameter.Promotions}}": {
				"id": "{{#? this.id}}",
				"name": "{{#? this.name}}",
				"description": "{{#? this.description}}",
				"type": "{{#? this.type}}",
				"percentDiscount": "{{#? this.percentDiscount}}",
				"dateStart": "{{#? this.dateStart}}",
				"dateEnd": "{{#? this.dateEnd}}",
				"imageUrl": "{{#? this.imageUrl}}",
				"status": "{{#? this.status}}",
				"createdAt": "{{#? this.createdAt}}",
				"updatedAt": "{{#? this.updatedAt}}"
			}
		}
	}
}