{
	"status": true,
	"message": {
		"code": "{{#? message.code}}",
		"text": "{{#? message.text}}"
	},
	"roles": {
		"{{#each roles}}": {
			"id": "{{#? this.id}}",
			"name": "{{#? this.name}}",
			"description": "{{#? this.description}}",
			"status": "{{#? this.status}}",
			"createdAt": "{{#? this.createdAt}}",
			"updatedAt": "{{#? this.updatedAt}}",
			"functions": {
				"{{#each Functions}}": {
					"id": "{{#? this.id}}",
					"parentId": "{{#? this.parentId}}",
					"title": "{{#? this.title}}",
					"description": "{{#? this.description}}",
					"entity": "{{#? this.entity}}",
					"action": "{{#? this.action}}",
					"urlIntranet": "{{#? this.urlIntranet}}",
					"urlMobile": "{{#? this.urlMobile}}"

				}
			}
		}
	}
}